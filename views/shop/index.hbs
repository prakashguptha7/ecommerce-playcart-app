
<div class="row">
    <div class="col-sm-6 col-md-4 col-md-offset-4 col-sm-offset-3">
        <div id="success" class="alert alert-success" {{#if noMessages}}hidden{{/if}}>
            {{ successMsg }}
        </div>
    </div>
</div>

{{# each products }}
    <div class="row">
        {{# each this }}
        <div class="col-sm-6 col-md-4">
            <div class="img-thumbnail img-fluid">
                <img class="image-responsive" src="{{this.imagePath}}" alt="...">
                <div class="caption">
                    <h3>{{ this.title }}</h3>
                    <p class="description">{{ this.description }}</p>
                    <div class="clearfix">
                        <div class="price float-left">${{ this.price }}</div>
                        <a href="/add-to-cart/{{this._id}}" class="btn btn-success float-right" role="button">Add to Cart</a>
                    </div>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
{{/each}}




